<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.resto.shop.web.dao.PosMapper">
    <select id="selectArticleSupport" resultType="com.resto.shop.web.posDto.ArticleSupport">
        select article_id as articleId, support_time_id as supportTimeId from tb_article_support_time
        where article_id in
        <foreach collection="articleList" item="article" open="("
                 separator="," close=")">
            #{article.id}
        </foreach>
    </select>
</mapper>
